ggplot(arrhythmia_clean, aes(y = Avg_QRS_duration_msec)) +
  geom_boxplot()
## Histogram
ggplot(arrhythmia_clean, aes(x = Avg_QRS_duration_msec)) + 
  geom_histogram(bins = 30)
####Skewed to the right.
## Outliers
boxplot.stats(arrhythmia_clean$Avg_QRS_duration_msec)$stats
boxplot.stats(arrhythmia_clean$Avg_QRS_duration_msec)$out
arrhythmia_clean %>%
  select(Age_yr, Arrhythmia_Class, Avg_QRS_duration_msec) %>%
  filter(Avg_QRS_duration_msec > 114)
###All are of LBBB & RBBB cases with 1 AFib and 1 sinus tachycardy 
arrhythmia_clean %>%
  select(Age_yr, Arrhythmia_Class, Avg_QRS_duration_msec) %>%
  filter(Avg_QRS_duration_msec < 60)
###Normal observation 


